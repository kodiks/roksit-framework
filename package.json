{
  "name": "roksit-framework",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-scss": "cp -r projects/roksit-lib/src/lib/assets dist/roksit-lib/assets",
    "copy-files": "cp README.md dist/roksit-lib",
    "pack-archive": "cd dist/roksit-lib && npm pack . && cp roksit-lib-0.*.tgz ../..",
    "remove-archive": "rm dist/roksit-lib/roksit-lib-0.*.tgz && cd ../..",
    "compile-lib": "ng build roksit-lib && npm run copy-scss && npm run copy-files && npm run pack-archive && npm run remove-archive",
    "schematics:build": "tsc -p projects/schematics/tsconfig.json",
    "schematics:copy": "npm run schematics:build && webpack",
    "compile": "npm run compile-lib && npm run schematics:copy",
    "build:watch": "ng build roksit-lib && npm run copy-scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ng-bootstrap/ng-bootstrap": "^5.1.5",
    "angular-feather": "^6.0.4",
    "angularx-flatpickr": "^6.2.0",
    "apexcharts": "^3.15.2",
    "bootstrap": "^4.4.1",
    "flatpickr": "^4.6.3",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^5.3.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.22",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
